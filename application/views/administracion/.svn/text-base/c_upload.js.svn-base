function GestionarAlbum1(){     
    $('#td_interior').children().remove();
	var upload = $.JCP.Crear('upload', {
                                          padre : $('#td_interior'),
										  id : 'Album1',
										  css : {width:540,height:620},
										  urls:{
										  	 listar : 'c_upload/DevolverImagenesAlbum1',
											 adicionar : 'c_upload/GuardarFotoAlbum1',
											 eliminar : 'c_upload/EliminarFotoAlbum1'
										  },
										  urlImagenes:'noticias/',							  
										  tieneCss: true
   										});
}

