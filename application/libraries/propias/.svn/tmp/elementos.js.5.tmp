function PasarParciales(obj , objFinal, atributos , inicio, fin) {
    for(var i = 0; i< atributos.length; i++){
	    if(obj[atributos[i]] != undefined)
		objFinal[atributos[i]] =  obj[atributos[i]].slice(inicio, fin);
	}
}

